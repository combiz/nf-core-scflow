{
  "QC": {
    "key_colname": "manifest",
    "factor_vars": [
      "capdate",
      "prepdate",
      "seqdate",
      "aplevel"
    ],
    "min_library_size": 250,
    "max_library_size": "adaptive",
    "min_features": 100,
    "max_features": "adaptive",
    "max_mito": 0.1,
    "min_ribo": 0,
    "max_ribo": 1,
    "min_counts": 2,
    "min_cells": 2,
    "drop_unmapped": true,
    "drop_mito": true,
    "drop_ribo": false,
    "nmads": 4.0
  },
  "findcells": {
    "find_cells": true,
    "lower": 100,
    "retain": 12000,
    "alpha_cutoff": 0.001,
    "niters": 30000
  },
  "singlets": {
    "find_singlets": true,
    "singlets_method": "doubletfinder",
    "vars_to_regress_out": [
      "null"
    ],
    "pca_dims": 10,
    "var_features": 2000,
    "doublet_rate": 0,
    "dpk": 8,
    "pK": 0.02
  },
  "integrate": {
    "method": "Liger",
    "unique_id_var": "manifest",
    "take_gene_union": false,
    "remove_missing": true,
    "num_genes": 3000,
    "combine": "union",
    "keep_unique": false,
    "capitalize": false,
    "use_cols": true,
    "k": 30,
    "lambda": 5.0,
    "thresh": 0.0001,
    "max_iters": 100,
    "nrep": 1,
    "rand_seed": 1,
    "knn_k": 20,
    "k2": 500,
    "prune_thresh": 0.2,
    "ref_dataset": "NULL",
    "min_cells": 2,
    "quantiles": 50,
    "nstart": 10,
    "resolution": 1,
    "dims_use": "NULL",
    "dist_use": "CR",
    "center": false,
    "small_clust_thresh": 0
  },
  "integration_report": {
    "categorical_covariates": [
      "individual", 
      "group",
      "diagnosis",
      "sex",
      "capdate",
      "prepdate",
      "seqdate"
    ],
    "input_reduced_dim": "UMAP"
  },
  "merge": {
    "plot_vars": [
      "total_features_by_counts",
      "total_counts",
      "pc_mito",
      "pc_ribo"
    ],
    "facet_vars": ["null"],
    "outlier_vars": [
      "total_features_by_counts",
      "total_counts"
    ]
  },
  "reddim": {
    "input_reduced_dim": [
      "PCA",
      "Liger"
    ],
    "reduction_methods": [
      "tSNE", 
      "UMAP", 
      "UMAP3D"
    ],
    "vars_to_regress_out": [
      "nCount_RNA", 
      "pc_mito"
    ],
    "umap": {
      "pca_dims": 30,
      "n_neighbors": 35,
      "n_components": 2,
      "init": "spectral",
      "metric": "euclidean",
      "n_epochs": 200,
      "learning_rate": 1,
      "min_dist": 0.4,
      "spread": 0.85,
      "set_op_mix_ratio": 1,
      "local_connectivity": 1,
      "repulsion_strength": 1,
      "negative_sample_rate": 5,
      "fast_sgd": false
    },
    "tsne": {
      "dims": 2,
      "initial_dims": 50,
      "perplexity": 150,
      "theta": 0.5,
      "stop_lying_iter": 250,
      "mom_switch_iter": 250,
      "max_iter": 1000,
      "pca_center": true,
      "pca_scale": false,
      "normalize": true,
      "momentum": 0.5,
      "final_momentum": 0.8,
      "eta": 1000,
      "exaggeration_factor": 12
    }
  },
  "cluster": {
    "cluster_method": "leiden",
    "reduction_method": "UMAP_Liger",
    "res": 0.001,
    "k": 50,
    "louvain_iter": 1
  },
  "mapct": {
    "clusters_colname": "clusters",
    "cells_to_sample": 10000
  },
  "de": {
    "de_method": [
      "MASTZLM"
    ],
    "mast_method": "bayesglm",
    "min_counts": 110,
    "min_cells_pc": 0.1,
    "rescale_numerics": true,
    "pseudobulk": true,
    "celltype_var": "cluster_celltype",
    "sample_var": "individual",
    "dependent_var": "group",
    "ref_class": "Control",
    "confounding_vars": [
      "sex", "age", "seqdate"
    ],
    "random_effects_var": "null",
    "fc_threshold": 1.1,
    "pval_cutoff": 0.05,
    "force_run": false
  },
   "IPA": {
    "reference_file": "null",
    "enrichment_tool": [
      "WebGestaltR"
    ],
    "enrichment_method": "ORA",
    "enrichment_database": [
      "GO_Biological_Process"
    ],
    "is_output": true,
    "output_dir": "."
  },
  "plot_reddim_genes": {
    "reduction_methods": [
      "UMAP_Liger"
    ]
  },
  "dirichlet": {
    "unique_id_var": "individual",
    "celltype_var": "cluster_celltype",
    "dependent_var": "group",
    "ref_class": "Control",
    "var_order": [
      "Control",
      "Low",
      "High"
    ]
  },
  "celltype_metrics": {
    "unique_id_var": "individual",
    "clusters_colname": "clusters",
    "celltype_var": "cluster_celltype",
    "facet_vars": [
      "manifest",
      "diagnosis",
      "sex",
      "capdate",
      "prepdate",
      "seqdate"
    ],
    "metric_vars": [
      "pc_mito",
      "pc_ribo",
      "total_counts",
      "total_features_by_counts"
    ]
  }
}
